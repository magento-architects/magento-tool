{
    "name": "magento/magento-tool",
    "description": "Magento Console Client",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.1",
        "ext-json": "*",
        "ext-curl": "*",
        "symfony/yaml": "^4.2",
        "illuminate/container": "^5.7",
        "illuminate/filesystem": "^5.7",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/config": "^5.7",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Console\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
